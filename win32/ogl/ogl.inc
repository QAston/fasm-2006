;------------------------------------------MAKRA-----------------------------------------------
 ;odkada na stos warto bezporedni typu float (DD)
 ;pushfl  0.45

pushfl	MACRO fl1
	db	68h
	dd	fl1
ENDM
;---------
 ;zwraca cos argumentu
 
cosfl	MACRO	arg1
 fld	arg1
 fcos
 fstp	arg1
ENDM
;---------
 ;zwraca sin argumentu

sinfl	MACRO	arg1
 fld	arg1
 fsin
 fstp	arg1
ENDM
;---------
 ;odklada na stos zmienna DD jako DQ
 ;pushdfl  zm

pushdf	MACRO _liczba_
	fld	dword ptr [_liczba_]
	sub	esp,8
	fstp	qword ptr [esp]
ENDM
;---------
 ;odkada na stos warto bezporedni typu double float (DQ)
 ;pushdfl  1.33333e0

pushdfl	MACRO liczba
LOCAL zmienna, Omin
	jmp	short Omin
	zmienna	dq	liczba
Omin:
	fld	qword ptr [&zmienna&]
	sub	esp,8
	fstp	qword ptr [esp]
ENDM
;---------
 ;mnozy liczbe przez czynnik, wynik do liczby
 ;czynnik pozostaje bez zmian
 
mulfl		MACRO		_liczba_,_czynnik_
	fld	_liczba_
	fmul	_czynnik_
	fstp	_liczba_
ENDM
;---------
 ;dzieli liczbe przez czynnik, wynik do liczby
 ;czynnik pozostaje bez zmian

divfl		MACRO		_liczba_,_czynnik_
	fld	_liczba_
	fdiv	_czynnik_
	fstp	_liczba_
ENDM
;---------
 ;dodaje do zmiennej warto innej zmiennej (DD DQ...)
 ;addfl  Kacik,Zmiana_Kacika

addfl		MACRO		double1, increment
	fld	double1
	fadd	increment
	fstp	double1
ENDM
;---------
 ;odejmuje od zmiennej warto innej zmiennej (DD DQ...)
 ;subfl  Kacik,Zmiana_Kacika

subfl		MACRO		double1, decrement
	fld	double1
	fsub	decrement
	fstp	double1
ENDM
